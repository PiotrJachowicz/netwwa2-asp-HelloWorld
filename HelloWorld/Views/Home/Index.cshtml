@using HelloWorld.Models
@model Address
@{
    ViewData["Title"] = "Home Page";
    var message = "Hello world!!";
}
<div class="jumbotron">
    <h1>@message</h1>
</div>

<div class="well">
    <div>Aktualny czas: @ViewBag.CurrentTime</div>

    @{
        var list = new List<string> { "string 1", "string 2" };

        foreach (var item in list)
        {
            <div>@item</div>
        }

        @:To jest podsumowanie
    }

    <div>City: @Model.CityName</div>
    <div>Street: @Model.StreetName</div>
    <div>Building number: @Model.BuildingNumber</div>
</div>